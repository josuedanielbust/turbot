{
  "name": "mvcs",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "description": "Services template - MVCS",
  "main": "./src/index.js",
  "author": "josuedanielbust",
  "license": "MIT",
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "NODE_ENV=development PORT=3001 nodemon ./src/server.js",
    "lint:check": "npx prettier --check \"src/**/*.js\"",
    "lint:format": "npx prettier --write \"src/**/*.js\"",
    "db:gen": "npx prisma generate",
    "db:mig": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "test": "jest --collectCoverage"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cors": "^8.2.0",
    "@prisma/client": "^4.12.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "fastify-plugin": "^4.5.0",
    "pino": "^8.11.0",
    "prisma": "^4.12.0"
  },
  "devDependencies": {
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "25.2.2",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.4.0",
    "prettier": "^2.5.1"
  }
}
